{
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "schedular/**/*",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "schedular/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/schedular/build/$1"
    }
  ]
}
